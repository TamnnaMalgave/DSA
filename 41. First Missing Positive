class Solution {
    public int firstMissingPositive(int[] nums) {
        int n=nums.length;
        int temp[]=new int[n+2];
        for(int i=0;i<n;i++)
        {
            if(nums[i]>0 && nums[i]<=n)
            temp[nums[i]]++;
        }
        for(int i=1;i<=n+1;i++)
        {
            if(temp[i]==0){
                return i;
            }
        }
        return 1;
    }
}
