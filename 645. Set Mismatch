class Solution {
    public int[] findErrorNums(int[] nums) {
        int missing=-1;
        int repeat=-1;
        int n=nums.length;
        int freq[]=new int[n+1];
        for(int i=0;i<n;i++)
        {
            freq[nums[i]]++;
        }
        for(int i=1;i<=n;i++)
        {
            if(freq[i]==0)
            {
                missing=i;
            }
            if(freq[i]==2)
            {
                repeat=i;
            }
        }
        return new int[]{repeat,missing};
    }
}
